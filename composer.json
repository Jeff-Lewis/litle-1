{
    "name": "petflow/litle",
    "description": "Litle integration for Petflow.com",
    "authors": [
        {
            "name": "Nate Krantz",
            "email": "nate@petflow.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nkrantz23/litle-sdk-for-php"
        }
    ],
    "version": "dev-master",
    "source": {
        "url": "git://github.com/petflow/litle",
        "type": "git",
        "reference": "master"
    },
    "require": {
        "nkrantz/litle-sdk-for-php" : "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "classmap": [
            "tests/UnitTestCase.php",
            "tests/FunctionalTestCase.php"
        ],
        "psr-0": {
            "Petflow\\Litle" : "src/"
        }
    }
}