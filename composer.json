{
    "name": "petflow/litle",
    "description": "An object oriented wrapper for the official PHP SDK for Litle.",
    "authors": [
        {
            "name": "Nate Krantz",
            "email": "nate@petflow.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "illuminate/support":        "4.1.*",
        "nkrantz/litle-sdk-for-php": "dev-master#1.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [
            "tests/UnitTestCase.php",
            "tests/FunctionalTestCase.php",
            "tests/CertificationTestCase.php",
            "src/Petflow/Litle/Exception/Exception.php"
        ],
        "psr-0": {
            "Petflow\\Litle" : "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nkrantz23/litle-sdk-for-php"
        }
    ]
}
